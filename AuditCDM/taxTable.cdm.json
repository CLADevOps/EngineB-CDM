        },
        {
          "name": "regulatorDefaultAccrualglAccountNumber",
          "purpose": "hasA",
          "dataType": "string",
          "isNullable": true,
          "displayName": "regulatorDefaultAccrualglAccountNumber",
          "description": "\"GL account used to reflect accruals due to\nregulator\"\n"
        },
        {
          "name": "regulatorDefaultExpenseglAccountNumber",
          "purpose": "hasA",
          "dataType": "string",
          "isNullable": true,
          "displayName": "regulatorDefaultExpenseglAccountNumber",
          "description": "\"GL account used to reflect accruals due to\nregulator\"\n"
        },
        {
          "name": "regulatorDefaultPayableglAccountNumber",
          "purpose": "hasA",
          "dataType": "string",
          "isNullable": true,
          "displayName": "regulatorDefaultPayableglAccountNumber",
          "description": "\"GL account used to reflect accruals due to\nregulator\"\n"
        },
        {
          "name": "regulatorIdentifier",
          "purpose": "hasA",
          "dataType": "string",
          "isNullable": true,
          "displayName": "regulatorIdentifier",
          "description": "\"Tax or other code assigned by Regulator for\nreporting organization to Regulator\"\n"
        },
        {
          "name": "regulatorName",
          "purpose": "hasA",
          "dataType": "string",
          "isNullable": true,
          "displayName": "regulatorName",
          "description": "\"Name of regulator for whom tax is withheld\nor accrued\"\n"
        },
        {
          "name": "regulatorRegion",
          "purpose": "hasA",
          "dataType": "string",
          "isNullable": true,
          "displayName": "regulatorRegion",
          "description": "\"Sub-region within country; in the U.S., this\nwould be state; in Canada it would be\nprovince.\"\n"
        },
        {
          "name": "regulatorReportingOrganization",
          "purpose": "hasA",
          "dataType": "string",
          "isNullable": true,
          "displayName": "regulatorReportingOrganization",
          "description": "\"Cross-reference to\nOrganization_Location_File_YYYYMMDD\"\n"
        },
        {
          "name": "regulatorRole",
          "purpose": "hasA",
          "dataType": "string",
          "isNullable": true,
