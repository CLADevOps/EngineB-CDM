{
  "jsonSchemaSemanticVersion": "1.0.0",
  "imports": [
    {
      "corpusPath": "idw:/foundations.cdm.json"
    }
  ],
  "definitions": [
    {
      "entityName": "taxTable",
      "extendsEntity": "CdmEntity",
      "hasAttributes": [
        {
          "name": "regulatorActiveFlag",
          "purpose": "hasA",
          "dataType": "string",
          "isNullable": true,
          "displayName": "regulatorActiveFlag",
          "description": "True (Active) or False (Inactive)\n"
        },
        {
          "name": "regulatorCode",
          "purpose": "identifiedBy",
          "dataType": "string",
          "displayName": "regulatorCode",
          "description": "\"A code used to refer to this regulator or\njurisdiction; used as key or cross-reference\nin files.\"\n"
        },
        {
          "name": "regulatorCountry",
          "purpose": "hasA",
          "dataType": "string",
          "isNullable": true,
          "displayName": "regulatorCountry",
