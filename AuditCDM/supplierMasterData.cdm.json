                    {
                        "name": "paymentDiscountDays",
                        "purpose": "hasA",
                        "dataType": "string",
                        "isNullable": true,
                        "displayName": "paymentDiscountDays",
                        "description": "The number of days from the invoice date\r\nthe supplier allows to take advantage of\r\ndiscounted terms. Terms are represented as\r\ndigits with no decimal places (for example,\r\nnnn).",
                        "appliedTraits": null
                    },
                    {
                        "name": "paymentDiscountPercentage",
                        "purpose": "hasA",
                        "dataType": "decimal",
                        "isNullable": true,
                        "displayName": "paymentDiscountPercentage",
                        "description": "The discount percentage the supplier may\r\nprovide if an invoice is paid before a certain\r\nnumber of days. In the flat file, terms are\r\nrepresented as digits to one decimal place\r\n(for example, 10% would be represented as\r\n10.0). In extensible business reporting\r\nlanguage global ledger taxonomy framework\r\n(XBRL GL), the three fields\r\nTerms_Discount_Percentage,\r\nTerms_Discount_Days and\r\nTerms_Due_Days would be entered in the\r\nform \"xx.x% dd Net dd,\" such as 2% 10 Net\r\n30 for 2% discount if paid within 10 days,\r\nwith the net due in 30 days.",
                        "appliedTraits": {
                            "traitReference": "is.dataFormat.numeric.shaped",
                            "arguments": [
                                {
                                    "name": "precision",
                                    "value": "19"
                                },
                                {
                                    "name": "scale",
                                    "value": "5"
                                }
                            ]
                        }
                    },
