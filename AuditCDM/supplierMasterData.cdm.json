            "hasAttributes": [
                [
                    {
                        "name": "accountId",
                        "purpose": "hasA",
                        "dataType": "string",
                        "isNullable": true,
                        "displayName": "accountId",
                        "description": "General Ledger Account code for this supplier. Can be including sub-account id. It can contain many different levels to identify the Account. It could include cost centres such as company, division, region, group and branch/department.",
